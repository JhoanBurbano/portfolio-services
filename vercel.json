{
  "version": 2,
  "builds": [
    { "src": "main.ts", "use": "@vercel/node18" }
  ],
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "/main.ts",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "X-My-Custom-Header": "custom-value"
      } 
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}